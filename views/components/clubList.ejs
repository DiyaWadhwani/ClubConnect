<h2>Clubs</h2>
<div class="row g-3">
  <% for (let club of clubs) { %>

  <div class="col-4">
    <div class="card p-1">
      <h5 class="card-title"><%= club.name %></h5>
      <div class="card-text">
        <div><%= club.startDate %></div>
        <div><%= club.description %> description</div>
        <div>
          <a
            class="btn btn-outline-primary"
            href="<%= '/references/' + club.clubID + '/edit' %>"
            >Edit</a
          >
          <a
            class="btn btn-outline-danger"
            href="<%= '/references/' + club.clubID + '/delete' %>"
            >Delete</a
          >
        </div>
      </div>
    </div>
  </div>

  <% } %> <%- include("./pagination.ejs", { currentPage, lastPage }) %>
</div>
